     <div id="wrapper">
        <!-- Sidebar -->
        <div id="sidebar-wrapper" style="background-color: rgba(0,0,0,0.3)">
            <form action="/result" method="post" id="musicform">
                
                <div style="position:absolute;left:37%;color:whitesmoke;top:6%">
                    <select id="menulist" name="music" form="musicform" class="btn btn-success">
                        <option value="rock">Rock</option>
                        <option value="rap">Rap</option>
                        <option value="metal">Metal</option>
                        <option value="soul">Soul</option>
                        <option value="pop">Pop</option>
                        <option value="country">Country</option>
                    </select>    
                </div>
                
                <div class="checkbox checkbox-primary" style="position:absolute;left:37%;color:whitesmoke;top:20%">
                    <input type="checkbox" name="m1" value="chest" id="checkbox1" class="checkbox"><label for="checkbox1"> Chest</label><br>
                    <input type="checkbox" name="m2" value="triceps" id="checkbox2" form="musicform"><label for="checkbox2"> Triceps</label><br>
                    <input type="checkbox" name="m3" value="biceps" id="checkbox3" form="musicform"><label for="checkbox3"> Biceps</label><br>
                    <input type="checkbox" name="m4" value="legs" id="checkbox4" form="musicform"><label for="checkbox4"> Legs</label><br>
                    <input type="checkbox" name="m5" value="shoulders" id="checkbox5" form="musicform"><label for="checkbox5"> Shoulders</label><br>
                    <input type="checkbox" name="m6" value="back" id="checkbox6" form="musicform"><label for="checkbox6"> Back</label><br>
                    <input type="checkbox" name="m7" value="cardio" id="checkbox7" form="musicform"><label for="checkbox7"> Cardio</label><br>
                    <input type="checkbox" name="m8" value="abs" id="checkbox8" form="musicform"><label for="checkbox8"> Abs</label><br>
                    <input style="top:125%;position:absolute;right:20%" type="submit" value="Submit" class="btn btn-success">
                </div>
            </form>
            
            <p class="text-muted" style="position:absolute; left:20%; bottom:0">Â© 2015 Workout Helper</p>
        </div>
